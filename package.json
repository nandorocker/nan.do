{
  "name": "nan.do",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all clean --parallel dev:*",
    "build": "run-s clean build:*",
    "dev:11ty-watch": "eleventy --watch",
    "dev:11ty-server": "eleventy-dev-server --dir=public",
    "dev:css": "tailwindcss --input ./src/assets/css/tailwind.css --output ./public/tailwind.css --watch --postcss",
    "build:11ty": "eleventy",
    "build:css": "tailwindcss --input ./src/assets/css/tailwind.css --output ./public/tailwind.css --postcss",
    "clean": "rimraf public"
  },
  "keywords": [],
  "author": "nandorocker",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-dev-server": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13"
  }
}
